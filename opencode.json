{
  "$schema": "https://opencode.ai/config.json",
  "default_agent": "maia",
  "model": "zai-coding-plan/glm-4.7",
  "permission": {
    "edit": "allow",
    "bash": "allow",
    "write": "allow",
    "webfetch": "allow"
  },
  "plugin": [
    "oh-my-opencode"
  ],
  "provider": {
    "zai-coding-plan": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Z.ai Paid Coding Plan",
      "options": {
        "baseURL": "https://api.z.ai/api/coding/paas/v4"
      }
    },
    "google": {
      "npm": "@ai-sdk/google",
      "name": "Gemini Engine (Native)"
    },
    "opencode": {
      "name": "OpenCode Models"
    },
    "openrouter": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "OpenRouter Global",
      "options": {
        "baseURL": "https://openrouter.ai/api/v1"
      }
    }
  },
  "mcp": {
    "filesystem": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ]
    },
    "git": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-git"
      ]
    },
    "vibekanban": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "vibe-kanban@latest",
        "--mcp"
      ]
    }
  },
  "agent": {
    "maia": {
      "description": "SUPREME ORCHESTRATOR.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "primary",
      "tools": {
        "vibekanban_*": true,
        "discord_*": true,
        "session": true,
        "read": true,
        "write": true,
        "bash": true,
        "skill": true,
        "grep": true,
        "glob": true,
        "todoread": true,
        "todowrite": true
      }
    },
    "sisyphus": {
      "description": "Project Manager.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "vibekanban_*": true,
        "session": true,
        "read": true,
        "write": true,
        "grep": true,
        "skill": true,
        "todoread": true,
        "todowrite": true
      }
    },
    "coder": {
      "description": "LSP Architect.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "filesystem_*": true,
        "git_*": true,
        "patch": true,
        "edit": true,
        "write": true,
        "read": true,
        "lsp": true,
        "bash": true,
        "skill": true
      }
    },
    "ops": {
      "description": "Infra & Scripts.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "bash": true,
        "read": true,
        "write": true,
        "edit": true,
        "webfetch": true,
        "skill": true
      }
    },
    "researcher": {
      "description": "Fast Intel.",
      "model": "google/gemini-2.5-flash",
      "mode": "subagent",
      "tools": {
        "webfetch": true,
        "read": true,
        "grep": true,
        "skill": true,
        "glob": true
      }
    },
    "researcher_deep": {
      "description": "Deep Oracle.",
      "model": "google/gemini-2.5-pro",
      "mode": "subagent",
      "tools": {
        "webfetch": true,
        "read": true,
        "grep": true,
        "glob": true,
        "skill": true
      }
    },
    "giuzu": {
      "description": "Reasoning Clone.",
      "model": "openrouter/deepseek/deepseek-r1-0528:free",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "session": true,
        "skill": true
      }
    },
    "vision": {
      "description": "Native Vision.",
      "model": "google/gemini-2.0-flash",
      "mode": "subagent",
      "tools": {
        "read": true,
        "webfetch": true,
        "session": true
      }
    },
    "reviewer": {
      "description": "Logic Audit.",
      "model": "opencode/big-pickle",
      "mode": "subagent",
      "tools": {
        "read": true,
        "grep": true,
        "lsp": true,
        "skill": true
      }
    },
    "workflow": {
      "description": "Automations.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "bash": true,
        "skill": true
      }
    },
    "opencode": {
      "description": "Platform Oracle.",
      "model": "google/gemini-2.5-flash",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "bash": true,
        "webfetch": true,
        "skill": true
      }
    },
    "starter": {
      "description": "Bootstrapper.",
      "model": "google/gemini-2.5-flash",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "bash": true,
        "skill": true
      }
    },
    "librarian": {
      "description": "Success Curator.",
      "model": "google/gemini-2.5-flash",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "skill": true,
        "glob": true
      }
    },
    "maia_premium": {
      "description": "Supreme Arbiter.",
      "model": "google/gemini-2.5-pro",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "edit": true,
        "bash": true,
        "session": true,
        "skill": true,
        "grep": true,
        "glob": true,
        "todoread": true,
        "todowrite": true,
        "vibekanban_*": true
      }
    },
    "prometheus": {
      "description": "Planner. Creates work plans and milestones.",
      "model": "openrouter/deepseek/deepseek-r1-0528:free",
      "mode": "subagent",
      "tools": {
        "read": true,
        "grep": true,
        "glob": true,
        "skill": true,
        "todowrite": true,
        "todoread": true
      }
    },
    "oracle": {
      "description": "Architect & Debugger. Deep system analysis.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "read": true,
        "grep": true,
        "glob": true,
        "lsp": true,
        "bash": true,
        "skill": true
      }
    },
    "explore": {
      "description": "Fast Scanner. Read-only codebase exploration.",
      "model": "google/gemini-2.5-flash",
      "mode": "subagent",
      "tools": {
        "read": true,
        "grep": true,
        "glob": true,
        "list": true
      }
    },
    "frontend": {
      "description": "UI/UX Engineer. Frontend development specialist.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "edit": true,
        "bash": true,
        "skill": true,
        "webfetch": true
      }
    },
    "sisyphus_junior": {
      "description": "Code Executor.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "read": true,
        "write": true,
        "edit": true,
        "bash": true,
        "lsp": true,
        "skill": true,
        "git_*": true
      }
    },
    "github": {
      "description": "GitHub Expert.",
      "model": "zai-coding-plan/glm-4.7",
      "mode": "subagent",
      "tools": {
        "read": true,
        "grep": true,
        "bash": true,
        "webfetch": true,
        "git_*": true
      }
    }
  },
  "command": {
    "init": {
      "description": "WAKEUP GOD MODE. Initialize the Dream Team.",
      "template": "Execute WAKEUP.sh protocol. Verify all 20 agents. Report status.",
      "agent": "maia"
    },
    "plan": {
      "description": "Create a Project Plan",
      "template": "Strategic Plan: \"$ARGUMENTS\". Delegate to @sisyphus + @prometheus.",
      "agent": "maia"
    },
    "ops": {
      "description": "Infrastructure Task",
      "template": "Infra Task: \"$ARGUMENTS\". Execute with @ops.",
      "agent": "ops"
    },
    "research": {
      "description": "Deep Research",
      "template": "Context: \"$ARGUMENTS\". Use @researcher_deep (Pro) for analysis.",
      "agent": "researcher_deep"
    },
    "audit": {
      "description": "Codebase Audit",
      "template": "Audit the code. Use @reviewer (Big Pickle) for logic check.",
      "agent": "reviewer"
    },
    "clone": {
      "description": "Consult Giuzu",
      "template": "Strategic Consult: \"$ARGUMENTS\". Use @giuzu (DeepSeek R1).",
      "agent": "giuzu"
    },
    "workflow": {
      "description": "Automation Architect",
      "template": "Workflow Task: \"$ARGUMENTS\". Use @workflow (GLM-4.7) for n8n/Flowise.",
      "agent": "workflow"
    }
  }
}