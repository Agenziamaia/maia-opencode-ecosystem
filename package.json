{
  "name": "maia-ecosystem",
  "private": true,
  "version": "1.0.0",
  "description": "The God-Tier Agentic Workspace",
  "type": "module",
  "scripts": {
    "init": "bash WAKEUP.sh",
    "plan": "opencode run plan",
    "ops": "opencode run ops",
    "research": "opencode run research",
    "audit": "opencode run audit",
    "clone": "opencode run clone",
    "heavy": "opencode run heavy",
    "track:handoffs": "node .opencode/scripts/auto-handoff.js track",
    "metrics:show": "node .opencode/scripts/auto-handoff.js metrics",
    "distill": "bash .opencode/scripts/distill_layer0.sh",
    "popout": "bash .opencode/scripts/generate_fresh_layer0.sh",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ecosystem": "vitest run .opencode/ecosystem/__tests__/ecosystem.test.ts",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "vitest": "^1.5.0",
    "tsx": "^4.7.0"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.1.36",
    "@modelcontextprotocol/sdk": "^0.5.0"
  }
}
