{
  "name": "whatsapp-agentic-bot",
  "version": "1.0.0",
  "description": "Production-ready agentic workflow for WhatsApp hotel bot on macOS",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:gateway": "node dist/gateway/index.js",
    "start:workers": "node dist/workers/index.js",
    "start:scheduler": "node dist/scheduler/index.js",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:reset": "tsx scripts/reset.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "logs:tail": "tail -f logs/app.log",
    "logs:health": "tail -f logs/health.log",
    "logs:error": "tail -f logs/error.log",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "agent",
    "workflow",
    "hotel",
    "automation"
  ],
  "author": "MAIA OPS",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.10.0",
    "bullmq": "^5.1.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "better-sqlite3": "^11.5.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "imapflow": "^1.0.158",
    "mailparser": "^3.6.5",
    "openai": "^4.20.1",
    "zod": "^3.22.4",
    "ulid": "^2.3.0",
    "p-queue": "^7.4.1",
    "p-retry": "^6.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
